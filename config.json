{
  "description": "Docker volume plugin for Telepresence",
  "documentation": "https://docs.docker.com/engine/extend/plugins/",
  "entrypoint": [
    "/sbin/tini", "--",
    "/bin/docker-volume-telemount"
  ],
  "env": [
    {
      "name": "DEBUG",
      "settable": [
        "value"
      ],
      "value": "false"
    },
    {
      "name": "DISCONNECTED_LINGER_TIME",
      "settable": [
        "value"
      ],
      "value": "5s"
    },
    {
      "name": "HOST_PING_TIMEOUT",
      "settable": [
        "value"
      ],
      "value": "1s"
    }
  ],
  "interface": {
    "socket": "telemount.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  },
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN"
    ],
    "devices": [
      {
        "path": "/dev/fuse"
      }
    ]
  },
  "network": {
    "type": "host"
  },
  "propagatedmount": "/mnt/volumes"
}
